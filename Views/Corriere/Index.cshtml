
@{
    ViewData["Title"] = "Corriere";
}

@model CorriereDashboardViewModel

<h1 class="title"> Benvenuto, </h1>
<h2 class="title"> questa &egrave; la tua dashboard.</h2>

<div class="row mt-3">
    <div class="col-md-4">
        <a class="btn btn btn-outline-primary" asp-action="Modifica" asp-controller="Corriere" asp-route-id="@Model.CorriereId"><i class="fas fa-user-edit"></i> Modifica account </a>
    </div>
   
</div>



<div class="row mt-3">
    <div class="col-md-4">
        <a class="btn btn btn-outline-primary" asp-action="NonAssegnati" asp-controller="Corriere" asp-route-id="@Model.CorriereId"><i class="fas fa-dolly-flatbed"></i> Ordini non assegnati </a>
    </div>
    <div class="col-md-4">
        <a class="btn btn btn-outline-primary" asp-action="Consegna" asp-controller="Corriere" asp-route-id="@Model.CorriereId"><i class="far fa-calendar-check"></i> Ordini da consegnare </a>
    </div>
     <div class="col-md-4">
        <a class="btn btn btn-outline-primary" asp-action="Cronologia" asp-controller="Corriere" asp-route-id="@Model.CorriereId"><i class="fas fa-history"></i> Cronologia ordini </a>
    </div>
</div>