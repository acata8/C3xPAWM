{
  "ConnectionsStrings": {
    "Default": "Data Source=Data/C3PAWM.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "MemoryCache": {
    "SizeLimit": 1000,
    "TimeExpirationCache": 60
  },
  "ResponseCache":{
    "Home":{
      "Location": "Client",
      "Duration": 60
      
    },
    "Elenco":{
      "Location": "Client",
      "Duration": 60
    }
  },
  
  "Negozi": {
      "PerPage": 5,
      "Order": {
        "By": "Name",
        "Ascending": false,
        "Allow": [
          "Nome",
          "Citta"
        ]
    }
  }
  
  
}
